{
  "name": "keycloak-angular",
  "version": "6.1.0",
  "description": "Easy Keycloak setup for Angular applications",
  "scripts": {
    "contributors:add": "node ./node_modules/all-contributors-cli/dist/cli add",
    "contributors:generate": "node ./node_modules/all-contributors-cli/dist/cli generate",
    "clean": "node ./node_modules/shx/lib/cli rm -rf ./dist/",
    "build": "npm run clean && node ./node_modules/@angular/cli/bin/ng build keycloak-angular --prod",
    "test": "node ./node_modules/@angular/cli/bin/ng test keycloak-angular",
    "test:ci": "node ./node_modules/@angular/cli/bin/ng test keycloak-angular --watch=false",
    "lint": "node ./node_modules/@angular/cli/bin/ng lint",
    "doc:serve": "node ./node_modules/docsify-cli/bin/docsify serve ./docs --port 3001"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/mauriciovigolo/keycloak-angular.git"
  },
  "author": {
    "name": "Mauricio Gemelli Vigolo",
    "email": "@mauriciovigolo",
    "url": "https://github.com/mauriciovigolo"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mauriciovigolo/keycloak-angular/issues"
  },
  "homepage": "https://github.com/mauriciovigolo/keycloak-angular#readme",
  "keywords": [
    "angular",
    "angular 4",
    "angular 5",
    "angular 6",
    "angular 7",
    "authentication",
    "authorization",
    "keycloak",
    "keycloak-js"
  ],
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular-devkit/build-ng-packagr": "~0.13.0",
    "@angular/cli": "~7.3.0",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/http": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "all-contributors-cli": "^5.10.2",
    "codelyzer": "~4.5.0",
    "core-js": "^2.5.4",
    "docsify-cli": "^4.3.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "keycloak-js": "^6.0.1",
    "ng-packagr": "^4.2.0",
    "protractor": "~5.4.0",
    "rxjs": "^6.3.3",
    "shx": "^0.3.2",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.34.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2",
    "zone.js": "^0.8.26"
  }
}
