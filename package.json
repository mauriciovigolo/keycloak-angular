{
  "name": "keycloak-angular",
  "version": "8.3.0",
  "description": "Easy Keycloak setup for Angular applications",
  "scripts": {
    "contributors:add": "node ./node_modules/all-contributors-cli/dist/cli add",
    "contributors:generate": "node ./node_modules/all-contributors-cli/dist/cli generate",
    "clean": "node ./node_modules/shx/lib/cli rm -rf ./dist/",
    "build": "npm run clean && node ./node_modules/@angular/cli/bin/ng build keycloak-angular --configuration production && node ./node_modules/shx/lib/cli cp ./README.md ./dist/keycloak-angular/",
    "test": "node ./node_modules/@angular/cli/bin/ng test keycloak-angular",
    "lint": "node ./node_modules/@angular/cli/bin/ng lint"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/mauriciovigolo/keycloak-angular.git"
  },
  "author": {
    "name": "Mauricio Gemelli Vigolo",
    "email": "@mauriciovigolo",
    "url": "https://github.com/mauriciovigolo"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mauriciovigolo/keycloak-angular/issues"
  },
  "homepage": "https://github.com/mauriciovigolo/keycloak-angular#readme",
  "keywords": [
    "angular",
    "authentication",
    "authorization",
    "keycloak",
    "keycloak-js"
  ],
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.0.0",
    "@angular-devkit/core": "^12.0.0",
    "@angular/cli": "^12.0.0",
    "@angular/common": "^12.0.0",
    "@angular/compiler": "^12.0.0",
    "@angular/compiler-cli": "^12.0.0",
    "@angular/core": "^12.0.0",
    "@angular/language-service": "^12.0.0",
    "@angular/platform-browser": "^12.0.0",
    "@angular/platform-browser-dynamic": "^12.0.0",
    "@angular/router": "^12.0.0",
    "@types/jasmine": "^3.7.4",
    "@types/jasminewd2": "^2.0.9",
    "@types/node": "^15.3.0",
    "all-contributors-cli": "^6.20.0",
    "codelyzer": "^6.0.2",
    "jasmine-core": "^3.7.1",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.6.0",
    "keycloak-js": "^15.0.0",
    "ng-packagr": "^12.0.0",
    "protractor": "~7.0.0",
    "puppeteer": "^9.1.1",
    "rxjs": "^6.6.7",
    "shx": "^0.3.3",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "typescript": "~4.2.4",
    "zone.js": "~0.11.4"
  },
  "dependencies": {
    "tslib": "^2.2.0"
  }
}
